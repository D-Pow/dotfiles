[core]
    editor = subl -n -w
    eol = lf
    autocrlf = false
    ignorecase = false
    symlinks = true
[init]
    defaultBranch = master
[pull]
    rebase = false
[push]
    default = simple
[user]
    name = D-Pow
    email = D-Pow@users.noreply.github.com
[credential]
    helper = /usr/bin/git-credential-manager-core
    credentialStore = secretservice
